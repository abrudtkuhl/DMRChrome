{
  "manifest_version": 2,

  "name": "DMR Bypass Paywall",
  "description": "Browse DMR website without the annoying paywall notifications",
  "version": "0.8",

  "permissions": [
    "cookies", "tabs", "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["http://www.desmoinesregister.com/*"],
    "js": ["jquery-2.0.3.min.js", "front.js"],
    "css" : ["front.css"]
  }]
}